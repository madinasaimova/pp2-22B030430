   "source": [
    "import psycopg2\n",
    "from config import params\n",
    "\n",
    "db=psycopg2.connect(**params)\n",
    "\n",
    "current=db.cursor()\n",
    "\n",
    "sql=\"\"\"\n",
    "    SELECT * FROM PhoneBook;\n",
    "\"\"\"\n",
    "\n",
    "current.execute(sql)\n",
    "\n",
    "result=current.fetchall()\n",
    "\n",
    "print(result)\n",
    "\n",
    "current.close()\n",
    "db.commit()\n",
    "db.close()"
   ]
  }
